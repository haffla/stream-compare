# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

POST    /insert                     controllers.Application.insert

POST    /login                      controllers.auth.AuthController.login

GET     /login                      controllers.auth.AuthController.loginPage

GET     /affe                       controllers.auth.AuthController.affe

GET     /logout                     controllers.auth.AuthController.logout

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
